<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>  
      <class name="org.LexGrid.commonTypes.Properties" table="codingScheme" 
	entity-name="org.LexGrid.commonTypes.Properties$CodingScheme"> 
	<cache usage="read-write"/>
              
      <id column="codingSchemeName" type="string"/>
            
        <bag name="_propertyList" access="field" table="codingSchemeProp" lazy="false">  
			 <key>
			 	<column name="codingSchemeName"/>		  
			 </key>
			<one-to-many class="org.LexGrid.commonTypes.Property$CodingScheme"/>
		</bag>     
    </class>
   
</hibernate-mapping>

