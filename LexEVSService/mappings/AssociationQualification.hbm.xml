<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>  
        <class name="org.LexGrid.relations.AssociationQualification" table="entityAssnsToEQuals" lazy="false">        
       		<cache usage="read-write"/>
       		
        <composite-id class="org.LexGrid.LexBIG.caCore.hibernate.compositeKeys.MultiAttributesKey">
            <key-property name="multiAttributesKey"/>   
      	</composite-id>       
      	    
          <property name="_associationQualifier" access="field" insert="false" update="false" type="string" column="qualifierName" /> 
          
          <one-to-one name="_qualifierText" access="field" class="org.LexGrid.commonTypes.Text$AssociationQualification">
 			<formula>multiAttributesKey</formula>
       	 </one-to-one>     
    </class>
    
      <class name="org.LexGrid.commonTypes.Text" table="entityAssnsToEQuals" 
    		entity-name="org.LexGrid.commonTypes.Text$AssociationQualification">
    	<cache usage="read-write"/>
        <composite-id class="org.LexGrid.LexBIG.caCore.hibernate.compositeKeys.MultiAttributesKey">
       		<key-property name="multiAttributesKey"/>  
        </composite-id> 
        <property name="_content" access="field" update="false" insert="false" column="qualifierValue"/>  
    </class>
</hibernate-mapping>