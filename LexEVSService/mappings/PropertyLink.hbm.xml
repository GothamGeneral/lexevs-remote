<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.LexGrid.concepts.PropertyLink" table="entityPropertyLinks">
  
  <cache usage="read-write"/>
  
       <composite-id class="org.LexGrid.LexBIG.caCore.hibernate.compositeKeys.PropertyLinkKey">
        	<key-property name="codingSchemeName"/>     	
        	<key-property name="entityCodeNamespace"/>  	 
        	<key-property name="entityCode"/>
        	<key-property name="sourcePropertyId"/>   
        	<key-property name="link"/>  
        	<key-property name="targetPropertyId"/> 	  
        </composite-id>
        
        <property name="_sourceProperty" insert="false" update="false" access="field" type="string" column="sourcePropertyId" /> 
        <property name="_propertyLink" insert="false" update="false" access="field" type="string" column="link" /> 
        <property name="_targetProperty" insert="false" update="false" access="field" type="string" column="targetPropertyid" /> 
        
  </class>
</hibernate-mapping>