<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>  

        <class name="org.LexGrid.commonTypes.PropertyQualifier" table="entityPropertyMultiAttrib" 
    where="typeName = 'qualifier'" entity-name="org.LexGrid.commonTypes.PropertyQualifier$Entity">  
    
    <cache usage="read-write"/>
          
        <composite-id class="org.LexGrid.LexBIG.caCore.hibernate.compositeKeys.EntityPropertyMultiAttribKey">
            <key-property name="codingSchemeName"/>
            <key-property name="entityCodeNamespace"/>           
            <key-property name="entityCode"/>
            <key-property name="propertyId"/>            
            <key-property name="typeName"/>
            <key-property name="attributeValue"/>
            <key-property name="val1"/>
      </composite-id>           
        
          <property name="_propertyQualifierName" access="field" insert="false" update="false" type="string" column="typeName" />  
          <property name="_propertyQualifierType" access="field" insert="false" update="false" column="qualifierType" />      
          
          <many-to-one name="_value" access="field" class="org.LexGrid.commonTypes.Text$EntityPropertyQualifier" not-found="ignore">
  		  	<formula>codingSchemeName</formula> 
  		  	<formula>entityCodeNamespace</formula> 
  		  	<formula>entityCode</formula> 
  		  	<formula>propertyId</formula> 
  		  	<formula>typeName</formula> 
  		  	<formula>attributeValue</formula> 
  		  	<formula>val1</formula> 
 		  </many-to-one>        
    </class>
    
         <class name="org.LexGrid.commonTypes.Text" table="entityPropertyMultiAttrib" 
    		entity-name="org.LexGrid.commonTypes.Text$EntityPropertyQualifier">
    		
    		<cache usage="read-write"/>
    		
      		<composite-id class="org.LexGrid.LexBIG.caCore.hibernate.compositeKeys.EntityPropertyMultiAttribKey">
            	<key-property name="codingSchemeName"/>
            	<key-property name="entityCodeNamespace"/>           
            	<key-property name="entityCode"/>
            	<key-property name="propertyId"/>            
            	<key-property name="typeName"/>
            	<key-property name="attributeValue"/>
            	<key-property name="val1"/>
      		</composite-id>  
        	<property name="_content" access="field" update="false" insert="false" column="val1"/>  
    	</class>
</hibernate-mapping>